<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="API Test">
    <test name="API Tests on chrome" parallel="tests" thread-count="2">
        <parameter name="browser" value="chrome"></parameter>
        <groups>
            <run>
                <include name="API"/>
            </run>
        </groups>
        <packages>
            <package name="Tests"/>
        </packages>
    </test>
</suite>